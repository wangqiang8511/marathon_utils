{
    "id": "/chronos",
    "instances": 1,
    "cpus": 0.1,
    "mem": 512,
    "shell": "false",
    "cmd": "/usr/bin/chronos run_jar --http_port 8080 --master zk://master0.mes0.razerdata.com:2181,master1.mes0.razerdata.com:2181,master2.mes0.razerdata.com:2181/mes0 --zk_hosts master0.mes0.razerdata.com:2181,master1.mes0.razerdata.com:2181,master2.mes0.razerdata.com:2181 --cluster_name chronos",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "mesosphere/chronos:chronos-2.3.3-1.0.1.ubuntu1404-mesos-0.22.1-1.0.ubuntu1404",
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    },
    "healthChecks": [
    {
        "path": "/",
        "portIndex": 0,
        "protocol": "HTTP",
        "gracePeriodSeconds": 300,
        "intervalSeconds": 60,
        "timeoutSeconds": 20,
        "maxConsecutiveFailures": 3,
        "ignoreHttp1xx": false
    }
    ]
}
