{
    \"id\": \"/zkweb\",
    \"instances\": $NUM_INSTANCES,
    \"cpus\": $CPUS,
    \"mem\": $MEM,
    \"shell\": \"false\",
    \"cmd\": \"\",
    \"container\": {
        \"type\": \"DOCKER\",
        \"docker\": {
            \"image\": \"goodguide/zk-web\",
            \"network\": \"BRIDGE\",
            \"portMappings\": [
                {
                    \"containerPort\": 8080,
                    \"hostPort\": 0,
                    \"protocol\": \"tcp\"
                }
            ]
        }
    },
    \"healthChecks\": [
    {
        \"path\": \"/\",
        \"portIndex\": 0,
        \"protocol\": \"HTTP\",
        \"gracePeriodSeconds\": 300,
        \"intervalSeconds\": 60,
        \"timeoutSeconds\": 20,
        \"maxConsecutiveFailures\": 3,
        \"ignoreHttp1xx\": false
    }
    ]
}
